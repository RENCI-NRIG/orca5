<?xml version="1.0" encoding="UTF-8"?> 
<script xmlns="http://geni-orca.renci.org/DeleteQoSPolicy" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://geni-orca.renci.org/DeleteQoSPolicy DeleteQoSPolicy.xsd"> 
    <action name="DeleteQoSPolicy"> 
        <exchange> 
            <send>
                config t
            </send> 
            <expect>
                {$DefaultPrompt}
            </expect> 
        </exchange> 
        <exchange> 
            <send>
                policy-map {$QoSPolicyName}
            </send> 
            <expect timeout="2000">
                {$DefaultPrompt}
            </expect> 
        </exchange> 
        <exchange> 
            <send>
                no class class-default
            </send> 
            <expect timeout="2000">
                {$DefaultPrompt}
            </expect> 
        </exchange> 
        <exchange> 
            <send>
                exit
            </send> 
            <expect>
                {$DefaultPrompt}
            </expect> 
        </exchange> 
        <exchange> 
            <send>
                no policy-map {$QoSPolicyName}
            </send> 
            <expect timeout="3000">
                {$DefaultPrompt}
            </expect> 
        </exchange> 
        <exchange> 
            <send>
                end
            </send> 
            <expect>
                {$DefaultPrompt}
            </expect> 
        </exchange> 
    </action> 
</script>