<mapping xmlns="http://issg.cs.duke.edu/cod/plugins/beans">
    <mysql>
        <entry name="mysql">
            <attributes>
                <map from="slc_guid"
                     to="orca.shirako.kernel.Slice.PropertyGuid" />
                <map from="slc_name"
                     to="orca.shirako.kernel.Slice.PropertyName" />
                <map from="slc_type"
                     to="orca.shirako.kernel.Slice.PropertyType" />
                <map from="slc_resource_type"
                     to="orca.shirako.kernel.Slice.PropertyResourceType" />
                <map from="rsv_resid"
                     to="orca.shirako.kernel.Reservation.PropertyID" />
                <map from="rsv_category"
                     to="orca.shirako.kernel.Reservation.PropertyCategory" />
                <map from="prx_name"
                     to="orca.shirako.proxies.Proxy.PropertyProxyActorName" />
                <map from="clt_name"
                     to="orca.shirako.util.Client.PropertyName" />
                <map from="clt_guid"
                     to="orca.shirako.util.Client.PropertyGuid" />
                <map from="cfgm_type"
                     to="orca.shirako.plugins.config.ConfigurationMapping.PropertyType" />
                <map from="cfgm_path"
                     to="orca.shirako.plugins.config.ConfigurationMapping.PropertyFile" />
                <map from="act_name"
                     to="orca.shirako.core.Actor.PropertyName" />
                <map from="act_guid"
                     to="orca.shirako.core.Actor.PropertyGuid" />
                <map from="act_type"
                     to="orca.shirako.core.Actor.PropertyType" />

                <map from="usr_name" to="orca.manage.internal.User.PropertyLogin" />

                <map from="pkg_guid"
                     to="orca.extensions.internal.ExtensionPackage.PropertyId" />

                <map from="plg_local_id"
                     to="orca.extensions.internal.Plugin.PropertyPluginId" />
                <map from="plg_type"
                     to="orca.extensions.internal.Plugin.PropertyPluginType" />
                <map from="plg_actor_type"
                     to="orca.extensions.internal.Plugin.PropertyActorType" />

                <map from="mo_key"
                     to="orca.manage.internal.ManagementObject.PropertyID" />

            </attributes>
        </entry>
        <entry name="inventory">
            <attributes>
                <map from="inv_name" to="orca.shirako.core.Unit.UnitHostName" />
                <map from="inv_uid" to="orca.shirako.core.Unit.UnitID" />
                <map from="inv_state"
                     to="orca.shirako.core.Unit.UnitInternalState" />
                <map from="inv_control" to="orca.shirako.core.Unit.UnitControl" />
				<map from="inv_ip" to="orca.shirako.core.Unit.UnitInventoryIP" />
            </attributes>
        </entry>

    </mysql>
</mapping>
