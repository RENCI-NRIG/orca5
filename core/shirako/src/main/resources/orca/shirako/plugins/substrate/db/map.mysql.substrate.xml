<mapping xmlns="http://issg.cs.duke.edu/cod/plugins/beans">
    <mysql>
        <entry name="mysql">
            <attributes>
                <map from="slc_guid"
                     to="orca.shirako.api.ISlice.PropertyGuid" />
                <map from="slc_name"
                     to="orca.shirako.api.ISlice.PropertyName" />
                <map from="slc_type"
                     to="orca.shirako.api.ISlice.PropertyType" />
                <map from="slc_resource_type"
                     to="orca.shirako.api.ISlice.PropertyResourceType" />
                <map from="rsv_resid"
                     to="orca.shirako.api.IReservation.PropertyID" />
                <map from="rsv_category"
                     to="orca.shirako.api.IReservation.PropertyCategory" />
                <map from="rsv_state"
                     to="orca.shirako.api.IReservation.PropertyState" />
                <map from="rsv_joining"
                     to="orca.shirako.api.IReservation.PropertyStateJoined" />
                <map from="rsv_pending"
                     to="orca.shirako.api.IReservation.PropertyPending" />
                <map from="prx_name"
                     to="orca.shirako.proxies.Proxy.PropertyProxyActorName" />
                <map from="clt_name"
                     to="orca.shirako.util.Client.PropertyName" />
                <map from="clt_guid"
                     to="orca.shirako.util.Client.PropertyGuid" />
                <map from="cfgm_type"
                     to="orca.shirako.plugins.config.ConfigurationMapping.PropertyType" />
                <map from="cfgm_path"
                     to="orca.shirako.plugins.config.ConfigurationMapping.PropertyFile" />
            </attributes>
        </entry>
        <entry name="unit">
            <attributes>
                <map from="unt_uid" to="orca.shirako.core.Unit.UnitID" />
                <map from="unt_state" to="orca.shirako.core.Unit.UnitInternalState" />
				<map from="unt_slc_id" to="" ignoreFrom="true" />
				<map from="unt_rsv_id" to="" ignoreFrom="true" />
				<map from="unt_type" to="" ignoreFrom="true" />
				<map from="unt_id" to="" ignoreFrom="true" />
				<map from="unt_act_id" to="" ignoreFrom="true" />
            </attributes>
        </entry>
        <entry name="inventory">
            <attributes>
                <map from="inv_name" to="orca.shirako.core.Unit.UnitHostName" />
                <map from="inv_uid" to="orca.shirako.core.Unit.UnitID" />
                <map from="inv_state" to="orca.shirako.core.Unit.UnitInternalState" />
				<map from="inv_control" to="orca.shirako.core.Unit.UnitControl" />
				<map from="inv_ip" to="orca.shirako.core.Unit.UnitInventoryIP" />
            </attributes>
        </entry>
    </mysql>
</mapping>
